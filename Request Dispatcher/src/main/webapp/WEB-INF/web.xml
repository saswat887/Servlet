<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Request Dispatcher</display-name>
  <servlet>
  	<servlet-name>first</servlet-name>
  	<servlet-class>com.org.FirstServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>first</servlet-name>
  	<url-pattern>/form</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>second</servlet-name>
  	<servlet-class>com.org.SecondServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>second</servlet-name>
  	<url-pattern>/student</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>third</servlet-name>
  	<servlet-class>com.org.VerifyUser</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>third</servlet-name>
  	<url-pattern>/verify</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
   <welcome-file>Home.html</welcome-file>
  </welcome-file-list>
</web-app>